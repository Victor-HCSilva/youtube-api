{
    "name": "Mudar Cores",
    "short_name": "Cor",
    "version": "1.0.0",
    "manifest_version": 3,
    "description": "Extens√£o para mudar o estilos no youtube",
    "action": {
        "default_popup": "html_for_redAndBlue.html"
    },
    "icons":{
        "128":"icon/icon-128-full-black.png"
    },
    "content_scripts": [
        {
            "matches": ["https://www.youtube.com/*"],
            "js": ["background_redAndBlue.js"],
            "css": ["css_for_redAndBlue.css"]
        }
    ],
    "permissions": [
        "https://youtube.com/*"
    ],
    "background": {
        "service_worker": "background_redAndBlue.js"
    }
}


